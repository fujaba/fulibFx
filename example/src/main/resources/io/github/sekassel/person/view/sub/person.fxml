<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="CENTER" spacing="5.0" style="-fx-border-radius: 2; -fx-border-color: BLACK;" type="HBox" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.sekassel.person.controller.PersonController">
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
   <ImageView fx:id="image" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" smooth="false" />
   <VBox>
      <Label fx:id="firstName" text="Firstname" />
      <Label fx:id="lastName" text="Lastname" />
   </VBox>
   <Button fx:id="deleteButton" mnemonicParsing="false" text="Remove" />
</fx:root>
